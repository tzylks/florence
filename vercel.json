{
    "version": 2,
    "builds": [
        {
            "src": "app/**/*.[jt]s?(x)",
            "use": "@vercel/next"
        },
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}